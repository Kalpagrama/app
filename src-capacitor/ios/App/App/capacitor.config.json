{
  "appId": "app.kalpa",
  "appName": "Kalpagramma",
  "bundledWebRuntime": false,
  "npmClient": "yarn",
  "webDir": "www",
  "backgroundColor": "#22222222",
  "plugins": {
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    }
  },
  "server": {
    "iosScheme": "app.kalpa",
    "allowNavigation": [
      "kalpa.app",
      "*.kalpa.app",
      "kalpagramma.ru",
      "kalpagramma.com",
      "oauth.yandex.ru",
      "passport.yandex.ru",
      "oauth.vk.com",
      "accounts.google.com",
      "www.facebook.com"
    ],
    "url": "http://192.168.88.12:8484"
  }
}