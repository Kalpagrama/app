{
  "appId": "app.kalpa",
  "appName": "Kalpagramma",
  "bundledWebRuntime": false,
  "npmClient": "yarn",
  "webDir": "www",
  "backgroundColor": "#22222222",
  "plugins": {
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    }
  },
  "server": {
    "iosScheme": "app.kalpa",
    "allowNavigation": [
      "*.kalpa.app"
    ]
  },
  "android": {
    "overrideUserAgent": "custom"
  },
  "ios": {
    "overrideUserAgent": "custom"
  }
}