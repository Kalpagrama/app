{
  "name": "kalpagrama",
  "version": "4.0.99",
  "description": "Kalpagrama application",
  "productName": "Kalpagrama",
  "cordovaId": "app.kalpa",
  "capacitorId": "app.kalpa",
  "author": "kalpagrama <info@kalpagramma.ru>",
  "license": "ISC",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "quasar dev -m spa",
    "dev_spa": "quasar dev -m spa",
    "dev_pwa": "quasar dev -m pwa --content-base dist",
    "dev_android": "quasar dev -m capacitor -T android",
    "dev_ios": "quasar dev -m capacitor -T ios",
    "dev_bex": "quasar dev -m bex",
    "dev_ssr": "quasar dev -m ssr",
    "build": "quasar build -m pwa",
    "build_debug": "quasar build --debug -m pwa",
    "build_spa": "quasar build -m spa",
    "build_pwa": "quasar build -m pwa",
    "build_android": "quasar build -m capacitor -T android",
    "build_ios": "quasar build -m capacitor -T ios -ide",
    "deploy_ios": "cd src-capacitor/ios/App &&  fastlane testflight_lane",
    "deploy_android": "cd src-capacitor/android &&  fastlane deploy",
    "gql": "apollo schema:download -k ./src/api/graphql.schema.json && node src/api/possible_types.js",
    "icons": "icongenie generate -p .icongenie.profile.json",
    "tr": "i18next-scanner --config src/i18n/i18next-scanner.config.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "cross-env RUN_TESTS=true yarn dev",
    "now-build": "quasar build -m pwa",
    "now": "now"
  },
  "resolution Comments": {
    "uuid": "pouchdb7.2.2 (pouchdb-adapter-idb + rxdb) содержит зависимость uuid@8.1.0, который не совместим webpack5 Принудительно ставим 8.3.2"
  },
  "resolutions": {
    "uuid": "^8.3.2"
  },
