    "gql": "apollo schema:download -k ./src/schema/graphql.schema.json && node ./src/system/gql/possible_types.js",
    "tr": "i18next-scanner --config src/i18n/i18next-scanner.config.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"No test specified\" && exit 0",
    "now-build": "quasar build -m pwa",
    "now": "now"
