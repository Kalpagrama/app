{
  "version": 2,
  "name": "kalpagramma",
  "builds": [{
    "src": "package.json",
    "use": "@now/static-build"
  }],
  "routes": [
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/img/(.*)", "dest": "/img/$1" },
    { "src": "^/fonts/(.*)", "dest": "/fonts/$1" },
    { "src": "^/statics/(.*)", "dest": "/statics/$1" },
    { "src": "^/statics/icons/(.*)", "dest": "/statics/icons/$1" },
    { "src": "^/(.*).js", "dest": "/$1.js" },
    { "src": "^/(.*).json", "dest": "/$1.json" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
