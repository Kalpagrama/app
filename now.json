{
  "version": 2,
  "build": {
    "env": {
      "SERVICES_URL": "https://dev.kalpa.app/graphql",
      "SERVICES_URL_DEBUG": "https://dev.kalpa.app/graphql"
    }
  },
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/img/(.*)", "dest": "/img/$1" },
    { "src": "^/fonts/(.*)", "dest": "/fonts/$1" },
    { "src": "^/scripts/(.*)", "dest": "/scripts/$1" },
    { "src": "^/icons/(.*)", "dest": "/icons/$1" },
    { "src": "^/other/(.*)", "dest": "/other/$1" },
    { "src": "^/(.*).js", "dest": "/$1.js" },
    { "src": "^/(.*).json", "dest": "/$1.json" },
    { "src": "^/(.*).ico", "dest": "/$1.ico" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
